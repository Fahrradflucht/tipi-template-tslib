{
  "name": "tipi-template-tslib",
  "version": "0.1.1",
  "description": "This is a tipi-cli template",
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "repository": {
    "url": "https://github.com/clebert/tipi-template-tslib.git",
    "type": "git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "files": [
    "template/LICENSE",
    "template/README.md",
    "template/package.json",
    "template/tsconfig.json",
    "template/tslint.json",
    "template/.conventional-changelog-lintrc",
    "template/.cz-config.js",
    "template/.editorconfig",
    "template/.gitignore",
    "template/.node-version",
    "template/.npmignore",
    "template/.npmrc",
    "template/.nvmrc",
    "template/.travis.yml",
    "template/.vscode",
    "template/.vscode/extensions.json",
    "template/.vscode/settings.json"
  ],
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version"
  },
  "devDependencies": {
    "commitizen": "2.8.6",
    "conventional-changelog-lint": "1.1.0",
    "cz-customizable": "4.0.0",
    "husky": "0.11.9",
    "standard-version": "4.0.0"
  }
}
